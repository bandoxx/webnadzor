                                    <div class="flex justify-between border-b border-stroke px-6.5 py-4 dark:border-strokedark">
                                        <h3 class="font-medium text-black dark:text-white">
                                            Osnovne postavke
                                        </h3>
                                        <p class="cursor-pointer closeable-btn mb-0">
                                            <svg class="size-6" fill="none" stroke="currentColor"
                                                 stroke-width="1.5" viewBox="0 0 24 24"
                                                 xmlns="http://www.w3.org/2000/svg">
                                                <path d="M5 12h14" stroke-linecap="round" stroke-linejoin="round"/>
                                            </svg>
                                        </p>
                                    </div>
                                    <div class="form-closeable">
                                        <div class="p-6.5">
                                            <div class="mb-4.5 flex flex-col gap-6 xl:flex-row">
                                                <div class="w-full xl:w-1/3">
                                                    <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                        Naziv lokacije
                                                    </label>
                                                    <div class="group relative w-3/4">
                                                        <input class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-2 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                               placeholder="Lokacija" type="text"
                                                               data-maxchars="40"
                                                               name="device_name"
                                                               value="{{ device.name }}">
                                                        <div class="absolute block xl:hidden left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                            <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                            Naziv lokacije, <br> maksimalno 40 znakova
                                                        </div>
                                                        <div class="absolute hidden xl:block left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                            <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                            Naziv lokacije, maksimalno 40 znakova
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="w-full xl:w-1/3">
                                                    <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                        Lokacija senzora 1
                                                    </label>
                                                    <div class="group relative w-3/4">
                                                        <input class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-2 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                               placeholder="Senzor 1" type="text"
                                                               data-maxchars="50"
                                                               name="t1_location"
                                                               value="{{ device.entry1.t_location }}">
                                                        <div class="absolute left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                            <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                            Lokacija senzora 1,<br> maksimalno <br>50 znakova
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="w-full xl:w-1/3">
                                                    <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                        Broj SIM kartice
                                                    </label>
                                                    <div class="group relative w-3/4">
                                                        <input class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-2 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
