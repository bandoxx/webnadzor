{% extends 'v2/base.html.twig' %}

{% block head %}
    <link href="https://unpkg.com/leaflet/dist/leaflet.css" rel="stylesheet"/>
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://unpkg.com/leaflet/dist/leaflet.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"></script>
    <meta charset="UTF-8"/>
    <meta content="IE=edge" http-equiv="X-UA-Compatible"/>
    <meta content="width=device-width, initial-scale=1.0" name="viewport"/>
{% endblock %}

{% block body %}
    <body
            :class="{'dark text-bodydark bg-boxdark-2': darkMode === true}"
            x-data="{ page: 'dataTables', modalOpenEdit: false, modalOpenAddNew: false, modalOpenDelete: false, 'loaded': true, 'darkMode': true, 'stickyMenu': false, 'sidebarToggle': false, 'scrollTop': false }"
            x-init="
          darkMode = JSON.parse(localStorage.getItem('darkMode'));
          $watch('darkMode', value => localStorage.setItem('darkMode', JSON.stringify(value)))"
    >
    <!-- ===== Preloader Start ===== -->
    {% block preloader %}
        {{ parent() }}
    {% endblock %}
    <!-- ===== Preloader End ===== -->

    <!-- ===== Page Wrapper Start ===== -->
    <div class="flex h-screen overflow-hidden">
        <!-- ===== Sidebar Start ===== -->
        <aside
                :class="sidebarToggle ? 'translate-x-0' : '-translate-x-full'"
                class="absolute left-0 top-0 z-9999 flex h-screen w-72.5 flex-col overflow-y-hidden bg-black duration-300 ease-linear dark:bg-boxdark lg:static lg:translate-x-0"
                @click.outside="sidebarToggle = false"
        >
            <!-- SIDEBAR HEADER -->
            {% block sidebar_header %}
                {{ parent() }}
            {% endblock %}
            <!-- SIDEBAR HEADER -->

            <div
                    class="no-scrollbar flex flex-col overflow-y-auto duration-300 ease-linear"
            >
                <!-- Sidebar Menu -->
                <nav
                        class="mt-5 px-4 py-4 lg:mt-9 lg:px-6"
                        x-data="{selected: $persist('Dashboard')}"
                >
                    <!-- Menu Group -->
                    <div>
                        <h3 class="mb-4 ml-4 text-sm font-medium text-bodydark2">MENU</h3>
                        <!-- Support Group -->
                        <div>
                            <ul class="mb-6 flex flex-col gap-1.5">
                                <!-- Menu Item Messages -->
                                <li id="submitDeviceForm" class="cursor-pointer">
                                    <a
                                            class="group relative flex items-center gap-2.5 rounded-sm px-4 py-2 font-medium text-bodydark1 duration-300 ease-in-out hover:bg-graydark dark:hover:bg-meta-4"
                                    >
                                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                                             stroke-width="1.5" stroke="currentColor" class="size-6">
                                            <path stroke-linecap="round" stroke-linejoin="round"
                                                  d="M9 12.75 11.25 15 15 9.75M21 12a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z"/>
                                        </svg>
                                        Spremi promjene
                                    </a>
                                </li>
                                {% if app.user.permission > 3 %}
                                    <li class="cursor-pointer">
                                        <a href="{{ path('app_device_toggledeviceparser', { deviceId: device.id, clientId: client.id}) }}"
                                           class="group relative flex items-center gap-2.5 rounded-sm px-4 py-2 font-medium text-bodydark1 duration-300 ease-in-out hover:bg-graydark dark:hover:bg-meta-4">
                                            {% if device.parserActive %}
                                                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                                                     stroke-width="1.5" stroke="currentColor" class="size-6">
                                                    <path stroke-linecap="round" stroke-linejoin="round"
                                                          d="M15 12H9m12 0a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z"/>
                                                </svg>
                                                Isključi parser
                                            {% else %}
                                                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                                                     stroke-width="1.5" stroke="currentColor" class="size-6">
                                                    <path stroke-linecap="round" stroke-linejoin="round"
                                                          d="M12 9v6m3-3H9m12 0a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z"></path>
                                                </svg>
                                                Uključi parser
                                            {% endif %}
                                        </a>
                                    </li>
                                {% endif %}
                                <li class="cursor-pointer">
                                    <a
                                            href="{{ path('app_device_list', { clientId: client.id }) }}"
                                            class="group relative flex items-center gap-2.5 rounded-sm px-4 py-2 font-medium text-bodydark1 duration-300 ease-in-out hover:bg-graydark dark:hover:bg-meta-4"
                                    >
                                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                                             stroke-width="1.5" stroke="currentColor" class="size-6">
                                            <path stroke-linecap="round" stroke-linejoin="round"
                                                  d="M8.25 9V5.25A2.25 2.25 0 0 1 10.5 3h6a2.25 2.25 0 0 1 2.25 2.25v13.5A2.25 2.25 0 0 1 16.5 21h-6a2.25 2.25 0 0 1-2.25-2.25V15m-3 0-3-3m0 0 3-3m-3 3H15"/>
                                        </svg>
                                        Povratak
                                    </a>
                                </li>
                                <!-- Menu Item Messages -->
                            </ul>
                        </div>
                    </div>
                </nav>
                <!-- Sidebar Menu -->
            </div>
        </aside>

        <!-- ===== Sidebar End ===== -->

        <div
                class="relative flex flex-1 flex-col overflow-y-auto overflow-x-hidden"
        >
            <!-- ===== Header Start ===== -->
            {% block header %}
                {{ parent() }}
            {% endblock %}
            <!-- ===== Header End ===== -->

            <!-- ===== Main Content Start ===== -->
            <main>
                <!--            Form that reads all the data-->
                <form method="post" id="deviceForm">
                    <div class="mx-auto max-w-screen-2xl p-4 md:p-6 2xl:p-10">
                        <!-- Breadcrumb Start -->
                        <div
                                class="mb-6 flex flex-col gap-3 sm:flex-row sm:items-center sm:justify-between"
                        >
                            <nav>
                                <ol class="flex items-center gap-2">
                                    <li>
                                        <a class="font-medium" href="{{ path('admin_overview') }}">Dashboard /</a>
                                    </li>
                                    <li>
                                        <a class="font-medium"
                                           href="{{ path('client_overview', { clientId: client.id }) }}">{{ client.name }}
                                            /</a>
                                    </li>
                                    <li>
                                        <a class="font-medium"
                                           href="{{ path('app_device_list', { clientId: client.id}) }}">Pregled mjernih
                                            uređaja /</a>
                                    </li>
                                    <li class="font-medium text-primary">Izmene uređaja</li>
                                </ol>
                            </nav>
                        </div>
                        <!-- Breadcrumb End -->
                        <div class="flex flex-col gap-5 md:gap-7 2xl:gap-10">
                            <div class="flex flex-col gap-9">
                                <!-- Basic Data Form -->
                                <div class="rounded-sm border border-stroke bg-white shadow-default dark:border-strokedark dark:bg-boxdark">
                                    <div class="flex justify-between border-b border-stroke px-6.5 py-4 dark:border-strokedark">
                                        <h3 class="font-medium text-black dark:text-white">
                                            Osnovne postavke
                                        </h3>
                                        <p class="cursor-pointer closeable-btn mb-0">
                                            <svg class="size-6" fill="none" stroke="currentColor"
                                                 stroke-width="1.5" viewBox="0 0 24 24"
                                                 xmlns="http://www.w3.org/2000/svg">
                                                <path d="M5 12h14" stroke-linecap="round" stroke-linejoin="round"/>
                                            </svg>
                                        </p>
                                    </div>
                                    <div class="form-closeable">
                                        <div class="p-6.5">
                                            <div class="mb-4.5 flex flex-col gap-6 xl:flex-row">
                                                <div class="w-full xl:w-1/2">
                                                    <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                        Naziv lokacije
                                                    </label>
                                                    <div class="group relative w-3/4">
                                                        <input class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-2 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                               placeholder="Lokacija" type="text"
                                                               data-maxchars="40"
                                                               name="device_name"
                                                               value="{{ device.name }}">
                                                        <div class="absolute block xl:hidden left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                            <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                            Naziv lokacije, <br> maksimalno 40 znakova
                                                        </div>
                                                        <div class="absolute hidden xl:block left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                            <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                            Naziv lokacije, maksimalno 40 znakova
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="w-full xl:w-1/2">
                                                    <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                        Lokacija senzora 1
                                                    </label>
                                                    <div class="group relative w-3/4">
                                                        <input class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-2 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                               placeholder="Senzor 1" type="text"
                                                               data-maxchars="50"
                                                               name="t1_location"
                                                               value="{{ device.entry1.t_location }}">
                                                        <div class="absolute left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                            <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                            Lokacija senzora 1,<br> maksimalno <br>50 znakova
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="mb-4.5 flex flex-col gap-6 xl:flex-row">
                                                <div class="w-full xl:w-1/2">
                                                    <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                        Naziv XML datoteke
                                                    </label>
                                                    <div class="group relative w-3/4">
                                                        <input class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-2 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                               placeholder="XML Datoteka" type="text"
                                                               data-maxchars="10"
                                                               name="xml_name"
                                                               value="{{ device.xmlName }}">
                                                        <div class="absolute block xl:hidden left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                            <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                            XML naziv,<br> maksimalno 18 znakova
                                                        </div>
                                                        <div class="absolute hidden xl:block left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                            <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                            XML naziv, maksimalno 18 znakova
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="w-full xl:w-1/2">
                                                    <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                        Lokacija senzora 2
                                                    </label>
                                                    <div class="group relative w-3/4">
                                                        <input class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-2 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                               placeholder="Senzor 2" type="text"
                                                               data-maxchars="50"
                                                               name="t2_location"
                                                               value="{{ device.entry2.t_location }}">
                                                        <div class="absolute left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                            <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                            Lokacija senzora 2, <br>maksimalno <br>50 znakova
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="mb-4.5 relative flex flex-col gap-6 xl:flex-row">
                                                <div class="w-full xl:w-1/3" id="showMap">
                                                    <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                        Mapa - lokacija
                                                    </label>
                                                    <input id="trueMapVals" type="hidden">
                                                    <input class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-2 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                           id="deviceMapCoordinates" placeholder="Lokacija na mapi"
                                                           type="text"
                                                           name="location"
                                                           value="{{ device.latitude }},{{ device.longitude }}">
                                                    <div class="bg-white absolute p-1 shadow-lg top-1/4 left-1/3"
                                                         id="mapContainer">
                                                        <div class="cursor-crosshair h-50 w-70"
                                                             id="deviceMap">
                                                            <button class="bg-white hover:bg-gray absolute right-0 border-x border-y z-9999"
                                                                    id="closeDeviceMap">
                                                                <svg class="size-6" fill="none"
                                                                     stroke="black" stroke-width="1.5"
                                                                     viewBox="0 0 24 24"
                                                                     xmlns="http://www.w3.org/2000/svg">
                                                                    <path d="M6 18 18 6M6 6l12 12"
                                                                          stroke-linecap="round"
                                                                          stroke-linejoin="round"/>
                                                                </svg>
                                                            </button>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="w-full xl:w-1/2">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--End of Basic Data Form-->
                                <!-- Temperature Data Form -->
                                <div class="rounded-sm border border-stroke bg-white shadow-default dark:border-strokedark dark:bg-boxdark">
                                    <div class="flex justify-between border-b border-stroke px-6.5 py-4 dark:border-strokedark">
                                        <h3 class="font-medium text-black dark:text-white">
                                            Postavke senzora temperature
                                        </h3>
                                        <p class="cursor-pointer closeable-btn mb-0">
                                            <svg class="size-6" fill="none" stroke="currentColor"
                                                 stroke-width="1.5" viewBox="0 0 24 24"
                                                 xmlns="http://www.w3.org/2000/svg">
                                                <path d="M5 12h14" stroke-linecap="round" stroke-linejoin="round"/>
                                            </svg>
                                        </p>
                                    </div>
                                    <div class="form-closeable">
                                        <div class="p-6.5">
                                            <div class="mb-4.5 flex flex-col gap-6 xl:flex-row">
                                                <div class="w-full flex flex-col xl:w-1/2">
                                                    <div class="w-full">
                                                        <div class="flex items-center rounded-full" id="tempTags1">
                                                            <a class="inline-flex rounded-l-full border border-stroke px-2 py-1 text-black font-medium hover:border-primary hover:bg-primary hover:text-white dark:border-strokedark dark:text-white dark:hover:border-primary sm:px-6 {% if device.entry1.t_use %}active{% endif %}"
                                                               href="#">Koristi
                                                                ulaz "Temperatura 1"</a>
                                                            <a class="inline-flex rounded-r-full border border-stroke px-2 py-1 font-medium text-black hover:border-primary hover:bg-primary hover:text-white dark:border-strokedark dark:text-white dark:hover:border-primary sm:px-6 {% if device.entry1.t_show_chart %}active{% endif %}"
                                                               href="#">Prikaži
                                                                graf</a>
                                                            <input type="hidden" value="" name="t1_use"/>
                                                            <input type="hidden" value="" name="t1_show_chart"/>
                                                        </div>
                                                        <div class="hidden" id="temp1Div">
                                                            <div class="w-3/4 mt-3">
                                                                <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                                    Naziv ulaza
                                                                </label>
                                                                <div class="group relative">
                                                                    <input class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-2 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                                           placeholder="Naziv ulaza"
                                                                           type="text"
                                                                           data-maxchars="16"
                                                                           name="t1_name"
                                                                           value="{{ device.entry1.t_name }}">
                                                                    <div class="absolute block xl:hidden left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                                        <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                                        Naziv za <br> "Temperatura 1" ulaz, <br>
                                                                        maksimalno 16
                                                                        znakova
                                                                    </div>
                                                                    <div class="absolute hidden xl:block left-full pointer-events-none top-1/2 z-20 ml-3 -translate-y-1/2 whitespace-nowrap rounded bg-black px-4.5 py-1.5 text-sm font-medium text-white opacity-0 group-hover:opacity-100">
                                                                        <span class="absolute left-[-3px] top-1/2 -z-10 h-2 w-2 -translate-y-1/2 rotate-45 rounded-sm bg-black"></span>
                                                                        Naziv za "Temperatura 1" ulaz, <br> maksimalno
                                                                        16
                                                                        znakova
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="w-3/4 mt-3">
                                                                <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                                    Ograničenje vrednosti
                                                                </label>
                                                                <input class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-2 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                                       placeholder="Ograničenje vrednosti"
                                                                       name="t1_note"
                                                                       type="text"
                                                                       value="{{ device.entry1.t_note ?? '' }}"
                                                                >
                                                            </div>
                                                            <div class="w-3/4 mt-3 relative"
                                                                 x-data="{ isOptionSelected: false }">
                                                                <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                                    Ikona
                                                                </label>
                                                                <select id="t1-image"
                                                                        :class="isOptionSelected &amp;&amp; 'text-black dark:text-white'"
                                                                        @change="isOptionSelected = true"
                                                                        name="t1_image"
                                                                        class="relative select-option z-20 w-full appearance-none rounded border border-stroke bg-transparent py-2 pl-5 pr-12 outline-none transition focus:border-primary active:border-primary dark:border-form-strokedark dark:bg-form-input">
                                                                    <option class="text-body" value="0">Isključeno
                                                                    </option>
                                                                    {% for icon in icons %}
                                                                        <option class="text-body" value="{{ icon.id }}"
                                                                                {% if device.entry1.t_image == icon.id %}selected="selected"{% endif %}>{{ icon.title }}</option>
                                                                    {% endfor %}
                                                                </select>
                                                                <span class="absolute right-4 top-10 z-20">
                          <svg fill="none" height="24" viewBox="0 0 24 24" width="24"
                               xmlns="http://www.w3.org/2000/svg">
                            <g opacity="0.8">
                              <path clip-rule="evenodd"
                                    d="M5.29289 8.29289C5.68342 7.90237 6.31658 7.90237 6.70711 8.29289L12 13.5858L17.2929 8.29289C17.6834 7.90237 18.3166 7.90237 18.7071 8.29289C19.0976 8.68342 19.0976 9.31658 18.7071 9.70711L12.7071 15.7071C12.3166 16.0976 11.6834 16.0976 11.2929 15.7071L5.29289 9.70711C4.90237 9.31658 4.90237 8.68342 5.29289 8.29289Z"
                                    fill="#637381"
                                    fill-rule="evenodd"></path>
                            </g>
                          </svg>
                        </span>
                                                            </div>
                                                            <div class="w-3/4 mt-3 relative"
                                                                 x-data="{ isOptionSelected: false }">
                                                                <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                                    Mjerna jedinica
                                                                </label>
                                                                <select :class="isOptionSelected &amp;&amp; 'text-black dark:text-white'"
                                                                        @change="isOptionSelected = true"
                                                                        value="{{ device.entry1.t_unit }}"
                                                                        name="t1_unit"
                                                                        class="relative select-option z-20 w-full appearance-none rounded border border-stroke bg-transparent py-2 pl-5 pr-12 outline-none transition focus:border-primary active:border-primary dark:border-form-strokedark dark:bg-form-input">
                                                                    {% for temperature in temperature_type %}
                                                                        <option class="text-body"
                                                                                value="{{ temperature }}"
                                                                                {% if device.entry1.t_unit == temperature %}selected="selected"{% endif %}>{{ temperature }}</option>
                                                                    {% endfor %}
                                                                </select>
                                                                <span class="absolute right-4 top-10 z-20">
                          <svg fill="none" height="24" viewBox="0 0 24 24" width="24"
                               xmlns="http://www.w3.org/2000/svg">
                            <g opacity="0.8">
                              <path clip-rule="evenodd"
                                    d="M5.29289 8.29289C5.68342 7.90237 6.31658 7.90237 6.70711 8.29289L12 13.5858L17.2929 8.29289C17.6834 7.90237 18.3166 7.90237 18.7071 8.29289C19.0976 8.68342 19.0976 9.31658 18.7071 9.70711L12.7071 15.7071C12.3166 16.0976 11.6834 16.0976 11.2929 15.7071L5.29289 9.70711C4.90237 9.31658 4.90237 8.68342 5.29289 8.29289Z"
                                    fill="#637381"
                                    fill-rule="evenodd"></path>
                            </g>
                          </svg>
                        </span>
                                                            </div>
                                                            <div class="w-3/4 mt-3">
                                                                <label class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                                    Minimalna vrijednost
