                                                        data-class="flatpickr-right"
                                                        id="datesDatePhoneFrom"
                                                        name="date_from"
                                                        onkeydown="return false"
                                                        placeholder="dd.mm.yyyy"
{#                                                        value="{{ date_from ? date_from|date('d.m.Y') : '' }}"#}
                                                />

